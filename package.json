{
  "name": "fraction-unicode",
  "version": "1.0.1",
  "description": "String representation of a number in fractional form",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": "abranhe/fraction-unicode",
  "scripts": {
    "build": "tsc",
    "format": "xo --fix",
    "test": "jest && xo",
    "prepare": "yarn build"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [
    "fraction",
    "number",
    "string",
    "unicode"
  ],
  "author": {
    "name": "Abraham Hernandez",
    "email": "abraham@abranhe.com",
    "url": "https://abranhe.com"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.4.4",
    "xo": "^0.50.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "xo": {
    "space": true,
    "rules": {
      "import/extensions": "off"
    }
  }
}